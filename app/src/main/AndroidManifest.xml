<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.strs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/strslogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".CustomerLoginActivity" />
        <activity android:name=".CompanyLoginActivity" />
        <activity android:name=".CustomerRegisterActivity" />
        <activity android:name=".CompanyRegisterActivity" />
        <activity android:name=".CompanyAreaActivity" />
        <activity android:name=".CustomerAreaActivity" />
        <activity
            android:name=".Payment"
            android:label="@string/title_activity_payment"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>